    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/node-fetch": "^2.6.12",
    "jest": "^29.7.0",
    "puppeteer": "^24.10.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
